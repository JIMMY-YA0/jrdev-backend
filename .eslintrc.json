{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "semi": [2, "always"],
    "prefer-destructuring": ["error", { "object": true, "array": false }],
    "consistent-return": "error"
    "arrow-parens": "as-needed"
  }
}
